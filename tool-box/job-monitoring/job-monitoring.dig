schedule:
  minutes_interval>: 5

+job-monitoring:
    docker:
        image: "armtd/td-script:2019.09"
    py>: job-monitoring.monitoring
    _env:
        td_apikey: ${secret:td.apikey}
        td.apiserver: ${secret:td.apiserver}
        td_database: ${secret:td.database}
        td_table: ${secret:td.table}
