_export:
  td:
    database: monitoring
    table: td_job_queue

schedule:
  minutes_interval>: 5

+job-monitoring:
    docker:
        image: "armtd/td-script:2019.09"
    py>: job-monitoring.monitoring
    _env:
        td_apikey: ${secret:td.apikey}
        td_endpoint: ${secret:td.apiserver}
        td_database: ${td.database}
        td_table: ${td.table}
